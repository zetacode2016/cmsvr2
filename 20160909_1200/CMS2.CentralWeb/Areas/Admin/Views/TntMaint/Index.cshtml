</form>
@section scripts
{
    <script type="text/javascript">

        function Add(module, field) {
            window.location = '@Url.Action("Add", "TntMaint")' + '?mod=' + module + '&fld=' + field;
        }

        function Edit(controlId) {
            window.location = '@Url.Action("Edit", "TntMaint")' + '?id=' + $('select[name="' + controlId + '"] option:selected').val();
        }

        function Delete(controlId) {
            window.location = '@Url.Action("Delete", "TntMaint")' + '?id=' + $('select[name="' + controlId + '"] option:selected').val();
        }

        function AddFlight() {
            window.location = '@Url.Action("Add", "FlightInfo")';
        }

        function EditFlight(controlId) {
            window.location = '@Url.Action("Edit", "FlightInfo")' + '?id=' + $('select[name="' + controlId + '"] option:selected').val();
        }

        function DeleteFlight(controlId) {
            window.location = '@Url.Action("Delete", "FlightInfo")' + '?id=' + $('select[name="' + controlId + '"] option:selected').val();
        }

    </script>
}