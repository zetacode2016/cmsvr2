    <div class="row">
        <div class="col-md-12 col-md-pull-1">
            <div class="form-group">
                <div class="control-label col-md-2" style="width: 25%; padding-right: 5px">
                @Html.ActionLink("Update TNT Database", "Update", null, new { @class = "btn btn-default" })
                </div>
            </div>
        </div>
